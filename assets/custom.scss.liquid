/* ------------------------------------------------------------ *\
  Helpers
\* ------------------------------------------------------------ */

.visible-pocket {
  display: none !important;

  @media (max-width: 1007px) {
    display: block !important;
  }
}

.pt20 {
  padding-top: 20px;
}

/* ------------------------------------------------------------ *\
  Product
\* ------------------------------------------------------------ */

.product-single {
  display: flex;
  flex-flow: row wrap;
  padding-top: 60px;
  padding-left: 100px;

  @media (max-width: 1007px) {
    display: block;
    padding: 10px 0 0;

    .Container {
      padding: 0 15px;
    }
  }
}

.Product__Wrapper {
  max-width: 100%;
}

.Product__Info {
  padding-top: 0;
}

.product-single__gallery {
  flex: 1 1;
}

.product-single__slider {
  margin-bottom: 40px;

  @media (max-width: 1007px) {
    margin-bottom: 20px;
  }
}

.Product__SlideshowNavImage {
  margin-right: 10px;
  margin-bottom: 0 !important;
}

Product__SlideshowNavImage:last-child {
  margin-right: 0;
}

.product-single__thumbnails {
  @media (max-width: 1007px) {
    display: none;
  }
}

.flickity-page-dots {
  display: none;

  @media (max-width: 1007px) {
    display: block;
  }
}

.product-single__meta {
  @media (max-width: 1007px) {
    text-align: center;
  }
}

.product-single__content {
  flex: 0 0 560px;
  max-width: 560px;

  @media (max-width: 1007px) {
    max-width: 100%;

    .product-single__meta {
      display: none;
    }
  }
}

.product-single__title {
  margin-bottom: 5px;
  font: 300 20px/1.25 'Montserrat', sans-serif;
  color: #000;
  letter-spacing: 0.1em;
  text-transform: uppercase;

  .stamped-product-reviews-badge {
    display: block !important;
    margin-top: 10px;
  }

  .stamped-badge > .stamped-badge-caption[data-reviews][data-label][data-version="2"] > span {
    display: none !important;
  }

  .stamped-fa-star {
    color: #000 !important;

    &:before {
      font-size: 28px;
    }
  }

  @media (min-width: 1008px) {
    font-size: 24px;

    .stamped-badge-starrating {
      font-size: 19px;
    }
  }
}

.product-single__description {
  margin-bottom: 23px;
  font-family: 'Open Sans', sans-serif;

  ul {
    text-align: left;
  }

  @media (max-width: 1007px) {
    text-align: center;
    padding-left: 15px;
    padding-right: 15px;

    & > h3 {
      text-align: left;
    }
  }
}

.product-single__links {
  margin-top: 20px;
}

.ProductForm__AddToCart {
  visibility: visible !important;
}

.ProductForm__BundleTitle {
  margin-bottom: 8px;
  text-align: center;

  @media (min-width: 1008px) {
    text-align: left;
    font-size: 22px;
  }
}

.ProductForm__QuantitySelector {
  @media (max-width: 1007px) {
    text-align: center;
  }
}

.product-single__usp {
  margin-top: 15px;
  text-align: center;

  ul {
    display: flex;
    flex-flow: row wrap;
    list-style-type: none;
  }

  li {
    flex: 0 0 33.33%;
    max-width: 33.33%;
    margin-top: 30px;
  }

  figure {
    margin: 0;
  }

  img {
    margin-bottom: 10px;
  }

  figcaption {
    font: 400 15px/1.25 'Open Sans', sans-serif;
  }

  @media (max-media: 1008px) {
    figcaption {
      font-size: 17px;
    }
  }
}

.product-single__price {
  font-weight: 300;
  font-size: 20px;
  letter-spacing: 0.1em;
  color: #000;

  @media (min-width: 1008px) {
    font-size: 25px;
  }
}

.product-reviews {
  font-family: 'Open Sans', sans-serif;
  padding: 45px 100px 0;
  color: #000;

  @media (max-width: 767px) {
    padding-left: 0;
    padding-right: 0;
  }

  .product-reviews__title {
    margin-bottom: 30px;
    font: 300 19px/1.25 'Montserrat', sans-serif;
    letter-spacing: 0.1em;
    text-transform: uppercase;
    text-align: center;

    @media (max-width: 767px) {
      margin-bottom: 0;
    }
  }

  /* Stamped.io */

  .stamped-container {
    max-width: 650px !important;
  }

  #tab-questions,
  .stamped-summary .stamped-summary-actions,
  .stamped-reviews-filter-label {
    display: none !important;
  }

  .stamped-review {
    padding-top: 0 !important;
    margin-bottom: 40px !important;
  }

  .stamped-review-header-title {
    font: 300 19px/1.25 'Montserrat', sans-serif;
    letter-spacing: 0.1em;
    text-transform: uppercase;
  }

  .stamped-review-content {
    margin-bottom: 35px !important;
  }

  .stamped-review-footer .stamped-share-icon {
    position: relative;

    &:after {
      content: '';
      position: absolute;
      bottom: -2px;
      left: 15px;
      right: 0;
      height: 1px;
      background: #b5b5b5;
    }
  }

  .stamped-tabs,
  .stamped-review {
    border-top: 0 !important;
    border-bottom: 1px solid #d7d7d7 !important;
  }

