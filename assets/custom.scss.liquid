.product-tabs {
  font-family: 'Open Sans', sans-serif;
  color: #000;

  .product-tabs__head {
    background: #000;
    padding: 27px 0 9px;

    ul {
      max-width: 1300px;
      padding: 0 100px;
      list-style-type: none;
    }

    li {
      display: inline-block;
    }

    li.active,
    li:hover {
      .product-tabs__link {
        background: #fff;
        color: #000;
      }
    }
  }

  .product-tabs__link {
    display: inline-block;
    margin-bottom: 25px;
    padding: 8px 50px;
    font-size: 16px;
    color: #fff;
    text-decoration: none;
    border-radius: 20px;
    text-transform: uppercase;
  }

  .product-tabs__inner {
    max-width: 1300px;
    margin: 0 auto;
    padding: 0 100px;
  }

  .product-tabs__body {
    background: #e4e3e3;
    padding: 30px 0;
    font-size: 18px;
  }

  @media (max-width: 1007px) {
    .product-tabs__head {
      ul {
        text-align: center;
        padding: 0 50px;
      }
    }

    .product-tabs__link {
      padding: 0 15px;
      margin-bottom: 15px;
    }

    .product-tabs__inner {
      max-width: 500px;
      padding: 0;
    }

    .product-tabs__body {
      padding: 20px;
      font-size: 16px;
    }
  }

  @media (max-width: 767px) {
    .product-tabs__head {
      ul {
        padding: 0 10px;
      }
    }

    .product-tabs__link {
      font-size: 14px;
      padding: 0 10px;
    }
  }

  @media (max-width: 374px) {
    .product-tabs__head {
      ul {
        padding: 0 5px;
      }
    }

    .product-tabs__link {
      padding: 0 5px;
    }
  }
}

.bundle-radios {
  display: flex;
  flex-flow: row wrap;
  padding-left: 5px;

  .bundle-radio {
    flex: 0 0 105px;
    max-width: 105px;
    margin-right: 30px;
    margin-bottom: 15px;
  }

  a {
    display: block;
    text-align: center;
    font: 300 16px/1.25 'Montserrat', sans-serif;
    letter-spacing: 0.1em;
    color: #b2b2b2;
    text-transform: uppercase;
    text-decoration: none;
  }

  .bundle-radio--active,
  .bundle-radio:hover {
    a {
      color: #000;
    }

    figure {
      border-color: #000;
    }
  }

  figure {
    margin: 0 0 10px;
    border: 1px solid transparent;
    padding: 5px 4px;
    letter-spacing: 0.1em;

    img {
      border: 1px solid #ccc;
    }
  }

  @media (max-width: 1007px) {
    justify-content: center;
  }
}

.product-usp {
  background: #000;
  margin-bottom: 30px;
  padding: 60px 30px 20px;

  ul {
    max-width: 1000px;
    margin: 0 auto;
    display: flex;
    flex-flow: row wrap;
    list-style-type: none;
  }

  li {
    flex: 0 0 33.33%;
    max-width: 33.33%;
    margin-bottom: 40px;
    color: #fff;
    font: 400 16px/1.375 'Open Sans', sans-serif;
    letter-spacing: 0.1em;
    text-align: center;
    text-transform: uppercase;

    strong {
      font-size: 32px;
    }

    p {
      margin-bottom: 0;
    }
  }

  @media (max-width: 1007px) {
    margin-bottom: 0;
    padding: 50px 0 10px;

    li {
      flex: 0 0 100%;
      max-width: 100%;
      font-size: 15px;

      strong {
        font-size: 28px;
      }
    }
  }
}

@media (max-width: 768px) {
  button.ProductForm__AddToCart + .product-single__description h3 {
    margin-top: 10px;
  }
}

.acsb-trigger.acsb-ready[data-acsb="trigger"] {
  display: none !important;
}

.link-accessibility {
  font-size: 0;
  width: 64px !important;
  height: 36px;
  background-image: url('{{ "accessible.png" | asset_url }}');
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: center center;
}

.Cart__Checkout {
  margin-left: 16px;
  display: inline-flex !important;

  @media (max-width: 640px) {
    margin-left: 0;
  }
}

