    <div class="PageContainer">
      {% section 'announcement' %}
      {% section 'header' %}

      <main id="main" role="main">
        {%- render 'ordergroove_global' -%}{{ content_for_layout }}
      </main>

      {% section 'footer' %}

      <div id="loading-overlay"><div class="loader"></div>
      
      {% comment %} side cart - start {% endcomment %}
      {% include 'side_cart' %}
      <script>
        $(function(){
          // revy upsells callback - update the side cart when an upsell product is added
          window.revyUpsellDataLayer = window.revyUpsellDataLayer || [];
          window.revyUpsellDataLayer.push({onAddToCartCallbacks: function() {
            window.updateCartDesc(true);
          }});
        })
      </script>
      {% comment %} side cart - end {% endcomment %}
    </div>

    {%- comment -%}
